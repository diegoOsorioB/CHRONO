<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/menuprincipalG"
    android:layout_height="match_parent"
    tools:context=".MenuPrincipalGuardiaActivity">

    <Button
        android:id="@+id/unidades"
        android:layout_width="220dp"
        android:layout_height="80dp"
        android:backgroundTint="#03A9F4"
        android:text="Segunda Revision"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.098" />

    <Button
        android:id="@+id/trasnportista"
        android:layout_width="220dp"
        android:layout_height="80dp"
        android:backgroundTint="#03A9F4"
        android:text="Consulta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unidades"
        app:layout_constraintVertical_bias="0.157" />

    <Button
        android:id="@+id/modificaRe"
        android:layout_width="220dp"
        android:layout_height="80dp"
        android:backgroundTint="#03A9F4"
        android:text="Modificar Revision"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trasnportista"
        app:layout_constraintVertical_bias="0.193" />

</androidx.constraintlayout.widget.ConstraintLayout>